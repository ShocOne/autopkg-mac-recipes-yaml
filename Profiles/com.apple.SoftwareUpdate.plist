<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AllowPreReleaseInstallation</key>
	<false/>
	<key>ManagedDeferredInstallDelay</key>
	<integer>%DEFERRAL_DAYS_MINOR_UPDATES%</integer>
	<key>enforcedSoftwareUpdateMajorOSDeferredInstallDelay</key>
	<integer>%DEFERRAL_DAYS_MAJOR_UPDATES%</integer>
	<key>enforcedSoftwareUpdateMinorOSDeferredInstallDelay</key>
	<integer>%DEFERRAL_DAYS_MINOR_UPDATES%</integer>
	<key>enforcedSoftwareUpdateNonOSDeferredInstallDelay</key>
	<integer>%DEFERRAL_DAYS_APP_UPDATES%</integer>
	<key>forceDelayedMajorSoftwareUpdates</key>
	<true/>
	<key>forceDelayedSoftwareUpdates</key>
	<true/>
	<key>forceDelayedAppSoftwareUpdates</key>
	<true/>
</dict>
</plist>
